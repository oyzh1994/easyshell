<?xml version="1.0" encoding="UTF-8"?>
<?import cn.oyzh.easyshell.fx.mysql.record.MysqlRecordTableView?>
<?import cn.oyzh.fx.plus.controls.box.FXHBox?>
<?import cn.oyzh.fx.plus.controls.box.FXVBox?>
<?import cn.oyzh.fx.plus.controls.text.FXText?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<FXVBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="cn.oyzh.easyshell.tabs.mysql.query.MysqlQueryExplainTabController" flexWidth="100%" flexHeight="100%">
    <MysqlRecordTableView fx:id="recordTable" flexHeight="100% - 30" flexWidth="100%">
    </MysqlRecordTableView>
    <FXHBox realHeight="30" flexWidth="100%">
        <FXText fx:id="sql">
            <HBox.margin>
                <Insets left="10" top="10"/>
            </HBox.margin>
        </FXText>
        <FXText fx:id="used">
            <HBox.margin>
                <Insets left="10" top="10"/>
            </HBox.margin>
        </FXText>
        <FXText fx:id="count">
            <HBox.margin>
                <Insets left="10" top="10"/>
            </HBox.margin>
        </FXText>
    </FXHBox>
</FXVBox>
