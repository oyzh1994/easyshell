<?xml version="1.0" encoding="UTF-8"?>
<?import cn.oyzh.fx.plus.controls.box.FXVBox?>
<?import cn.oyzh.fx.plus.controls.table.FXTableColumn?>
<?import cn.oyzh.fx.plus.controls.table.FXTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<FXVBox xmlns:fx="http://javafx.com/fxml/1" flexWidth="100% - 15" flexHeight="100%"
        fx:controller="cn.oyzh.easyshell.tabs.zk.query.ShellZKQueryStatTabController">
    <FXTableView fx:id="statTable" flexWidth="100%" flexHeight="100% - 5">
        <columns>
            <FXTableColumn valueName="key" flexWidth="35%" text="%base.name"/>
            <FXTableColumn valueName="value" flexWidth="65% - 15" text="%base.value"/>
        </columns>
        <VBox.margin>
            <Insets left="5" top="5"/>
        </VBox.margin>
    </FXTableView>
</FXVBox>
