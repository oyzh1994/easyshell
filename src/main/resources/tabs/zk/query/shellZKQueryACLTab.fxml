<?xml version="1.0" encoding="UTF-8"?>
<?import cn.oyzh.fx.plus.controls.box.FXVBox?>
<?import cn.oyzh.fx.plus.controls.table.FXTableColumn?>
<?import cn.oyzh.fx.plus.controls.table.FXTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<FXVBox xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="cn.oyzh.easyshell.tabs.zk.query.ShellZKQueryACLTabController" flexWidth="100% - 15" flexHeight="100%">
    <FXTableView fx:id="aclTable" flexWidth="100%" flexHeight="100% - 5">
        <columns>
            <FXTableColumn valueName="param1" flexWidth="30%" text="%base.id"/>
            <FXTableColumn valueName="param2" flexWidth="30%" text="%base.schema"/>
            <FXTableColumn valueName="param3" flexWidth="40% - 15" text="%base.perms"/>
        </columns>
        <VBox.margin>
            <Insets left="5" top="5"/>
        </VBox.margin>
    </FXTableView>
</FXVBox>