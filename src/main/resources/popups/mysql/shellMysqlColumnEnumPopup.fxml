<?xml version="1.0" encoding="UTF-8"?>
<?import cn.oyzh.easyshell.fx.mysql.table.ShellMysqlColumnListView?>
<?import cn.oyzh.fx.gui.svg.glyph.AddSVGGlyph?>
<?import cn.oyzh.fx.gui.svg.glyph.CloseSVGGlyph?>
<?import cn.oyzh.fx.gui.svg.glyph.DeleteSVGGlyph?>
<?import cn.oyzh.fx.gui.svg.glyph.SubmitSVGGlyph?>
<?import cn.oyzh.fx.plus.controls.box.FXHBox?>
<?import cn.oyzh.fx.plus.controls.box.FXVBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<FXVBox xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="cn.oyzh.easyshell.popups.mysql.ShellMysqlColumnEnumPopupController" realWidth="320"
        realHeight="200">
    <ShellMysqlColumnListView flexWidth="100%" flexHeight="100% - 25" fx:id="listView">
    </ShellMysqlColumnListView>
    <FXHBox flexWidth="100%" realHeight="25">
        <SubmitSVGGlyph onMousePrimaryClicked="#submit" sizeStr="15,11">
            <HBox.margin>
                <Insets left="10" top="5"/>
            </HBox.margin>
        </SubmitSVGGlyph>
        <CloseSVGGlyph onMousePrimaryClicked="#close" sizeStr="11">
            <HBox.margin>
                <Insets left="15" top="5"/>
            </HBox.margin>
        </CloseSVGGlyph>
        <AddSVGGlyph sizeStr="14" onMousePrimaryClicked="#addRow">
            <HBox.margin>
                <Insets left="15" top="3"/>
            </HBox.margin>
        </AddSVGGlyph>
        <DeleteSVGGlyph sizeStr="14" onMousePrimaryClicked="#deleteRow">
            <HBox.margin>
                <Insets left="15" top="3"/>
            </HBox.margin>
        </DeleteSVGGlyph>
    </FXHBox>
</FXVBox>
